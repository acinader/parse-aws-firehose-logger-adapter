  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-node6": "^11.0.0",
    "codecov": "^3.0.0",
    "config": "^1.24.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.3",
